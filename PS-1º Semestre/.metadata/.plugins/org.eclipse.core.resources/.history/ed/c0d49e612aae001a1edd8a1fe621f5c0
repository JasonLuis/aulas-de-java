package br.com.fiap.model;

public class SegmentoModel {
	
	private long id;
	private String name;
	private ChatbotModel chatbotModel;
	
	public SegmentoModel() {
		// TODO Auto-generated constructor stub
	}

	public SegmentoModel(long id, String name, ChatbotModel chatbotModel) {
		super();
		this.id = id;
		this.name = name;
		this.chatbotModel = chatbotModel;
	}

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public ChatbotModel getChatbotModel() {
		return chatbotModel;
	}

	public void setChatbotModel(ChatbotModel chatbotModel) {
		this.chatbotModel = chatbotModel;
	}
	
	
	
}
